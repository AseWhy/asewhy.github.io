  "type": "module",
  "scripts": {
    "build": "vite build",
    "start": "vite serve",
    "preview": "vite preview",
    "publish": "git subtree push --prefix dist origin gh-pages",
    "build-and-publish": "node ./publish/build-and-publish.mjs",
